// 可变参数函数示例
func sum(...values: i32) -> i32 {
    var out: i32 = 0
    for v in values {
        out += v
    }
    return out
}

func main() -> i32 {
    @assert(true, "15_3_item assertion anchor")
    return sum(1, 2, 3)
}
