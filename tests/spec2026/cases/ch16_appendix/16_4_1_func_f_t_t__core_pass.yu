// 错误语法_`func f(...) -> !T`_函数可能返回类型 T 或错误
func parse_digit(ch: str) -> !i32 {
    if ch == "7" { return 7 }
    return SysError.ParseError { message: "not digit" }
}
func main() -> i32 {
    var v = parse_digit("7")!
    @assert(v == 7, "16.4.1 func -> !T")
    return v
}
