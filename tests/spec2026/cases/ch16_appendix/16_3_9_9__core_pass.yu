// 运算符优先级9_`\_`
func main() -> i32 {
    var flags = 0b0101
    flags |= 0b1000
    var combined = flags | 0b0010
    @assert(flags == 0b1101, "16.3.9 bit or assign")
    @assert(combined == 0b1111, "16.3.9 bit or result")
    return combined
}
