// 运算符优先级14_`orelse`_Optional 默认值
func main() -> i32 {
    var none_v: ?i32 = None
    var some_v: ?i32 = 6
    var a = none_v orelse 9
    var b = some_v orelse 9
    @assert(a == 9 && b == 6, "16.3.14 orelse")
    return a + b
}
