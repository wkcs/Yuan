// 错误语法_`func()`_调用可能出错的函数，如果出错则 panic
func maybe(flag: bool) -> !i32 {
    if flag { return 5 }
    return SysError.ParseError { message: "boom" }
}
func main() -> i32 {
    var ok = maybe(true)
    @assert(ok == 5, "16.4.3 call may panic on error")
    return ok
}
