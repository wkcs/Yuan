// 可变参数函数
func sum(...values: i32) -> i32 {
    var out: i32 = 0
    for v in values {
        out += v
    }
    return out
}

func main() -> i32 {
    var a = sum(1, 2, 3)
    var b = sum(4, 5, 6, 7)
    @assert(a == 6, "6.2.5 variadic first call")
    @assert(b == 22, "6.2.5 variadic second call")
    return a + b
}
