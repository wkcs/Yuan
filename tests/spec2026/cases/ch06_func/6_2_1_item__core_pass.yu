// 普通参数（默认不可变）
func double_then_add(x: i32, y: i32) -> i32 {
    var left = x * 2
    return left + y
}

func main() -> i32 {
    var result = double_then_add(3, 4)
    @assert(result == 10, "6.2.1 immutable normal parameters")
    return result
}
