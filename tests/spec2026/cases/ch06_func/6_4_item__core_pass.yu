// 泛型函数
func swap_pair<T>(a: T, b: T) -> (T, T) {
    return (b, a)
}

func main() -> i32 {
    var p: (i32, i32) = swap_pair<i32>(3, 9)
    @assert(p.0 == 9, "6.4 generic first item")
    @assert(p.1 == 3, "6.4 generic second item")
    return p.0 + p.1
}
