// 函数定义
func add(a: i32, b: i32) -> i32 {
    var out = a + b
    return out
}

func mul(a: i32, b: i32) -> i32 {
    return a * b
}

func main() -> i32 {
    var s = add(4, 5)
    var p = mul(3, 4)
    @assert(s == 9, "6.1 function definition sum")
    @assert(p == 12, "6.1 function definition product")
    return s + p
}
