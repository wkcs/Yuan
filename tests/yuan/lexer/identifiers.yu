// 内置函数标识符（以 @ 开头）
@import "std/io"
@export func public_api()
@inline func fast_function()
@deprecated func old_function()
@test func unit_test()

// 复杂的内置标识符
@compile_time_constant PI = 3.14159
@memory_layout struct PackedStruct
@error_handling func may_fail() -> !i32

// 标识符在不同上下文中的使用
struct 结构体名 {
    字段1: i32,
    field_2: str,
    третье_поле: bool,
}

func 函数名(参数1: i32, param_2: str) -> bool {
    var 局部变量 = 参数1
    return 局部变量 > 0
}

enum 枚举类型 {
    选项一,
    Option_Two,
    Третий_Вариант,
}

// 泛型标识符
func generic_function<T, U, 类型参数>(value: T) -> U {
    // 泛型函数体
}

struct Container<元素类型> {
    data: 元素类型,
}

// 模块和命名空间标识符
mod 模块名 {
    pub struct 公共结构 {}
    priv func 私有函数() {}
}

// 特殊情况：关键字后的标识符
var after_var = 1
func after_func() {}
struct after_struct {}

// 边界情况：单字符标识符
var a = 1
var b = 2
var x = 3
var y = 4
var z = 5

