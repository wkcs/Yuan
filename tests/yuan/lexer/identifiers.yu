// 带数字的标识符
var name1 = 1
var var2 = 2
var test123 = 3
var a1b2c3 = 4

// 下划线标识符
var _ = 1          // 通配符
var _private = 2   // 私有变量约定
var __internal = 3 // 内部变量约定
var value_ = 4     // 尾随下划线

// 长标识符
var very_long_identifier_name_that_should_be_valid = 1
var AnotherVeryLongIdentifierWithMixedCase = 2

// Unicode 标识符（中文）
var 变量名 = 1
var 函数名 = 2
var 类型名 = 3
var 中文标识符 = 4

// Unicode 标识符（其他语言）
