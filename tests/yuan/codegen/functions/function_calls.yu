// 函数调用测试

func add(a: i32, b: i32) -> i32 {
    return a + b
}

func multiply(a: i32, b: i32) -> i32 {
    return a * b
}

func test_simple_call() -> i32 {
    return add(3, 4)
}

func test_nested_call() -> i32 {
    return add(multiply(2, 3), multiply(4, 5))
}

func test_multiple_calls() -> i32 {
    var x: i32 = add(1, 2)
    var y: i32 = add(3, 4)
    return add(x, y)
}

func factorial(n: i32) -> i32 {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

func test_recursive() -> i32 {
    return factorial(5)
}

func main() {
}
